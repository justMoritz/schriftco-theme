{
  "apiVersion": 3,
  "name": "schriftco/text-demo",
  "title": "Text Demo Block",
  "category": "widgets",
  "icon": "editor-alignleft",
  "description": "A styled textarea to preview a font.",
  "supports": {
    "html": false
  },
  "attributes": {
